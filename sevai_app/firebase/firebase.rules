service cloud.firestore {
  match /databases/{database}/documents {

    match /{document=**} {
      // allow read, write: if request.auth.token.email.matches("firebase@flutterflow.io");
      // allow read, write: if request.auth != null;
      allow read, write: if false; 
    }
    match /volunteeringLogs/{logId} {
      allow read, delete: if request.auth.uid != null && resource.data.uid == /databases/(default)/documents/users/$(request.auth.uid);
      allow write, create: if request.auth.uid != null && request.resource.data.uid == /databases/(default)/documents/users/$(request.auth.uid);
    }
    match /users/{userId} {
      allow create, read, write, delete: if request.auth.uid == userId;
    }
		match /organizations/{orgId} {
    	allow create, read, write, delete: if true;
    }
		match /organizations/{orgId}/users/{userId} {
    	allow create, read, write, delete: if true;
    }
  }
}